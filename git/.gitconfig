[user]
  name = Nick Nichols
  email = em . ail

[color]
  ui = always

[color "branch"]
  current = green
  local = green
  remote = yellow

[color "diff"]
  frag = magenta
  meta = yellow
  new = green
  old = red

[color "diff-highlight"]
  oldNormal = red bold
  oldHighlight = "red bold 52"
  newNormal = "green bold"
  newHighlight = "green bold 22"

[color "status"]
  added = green
  changed = yellow
  untracked = red

[rerere]
  enabled = true

[pull]
	rebase = false

[branch]
	sort = -committerdate

[help]
	autocorrect = 1
